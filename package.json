{
  "name": "turbo-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "MCP-Nexus â€” a comprehensive demonstration of a full Model Context Protocol (MCP) server with universal access from various clients in a Next.js application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hirurg-lybitel/MCP-Nexus.git"
  },
  "homepage": "https://github.com/hirurg-lybitel/MCP-Nexus#readme",
  "author": "hirurg-lybitel",
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:s": "turbo build --filter web... && turbo dev --filter web...",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "type-check": "turbo run type-check",
    "clean": "del-cli node_modules && del-cli node_modules .turbo && del-cli \"apps/*/.next\" \"apps/*/.turbo\" \"apps/*/dist\" \"apps/*/build\" \"apps/*/.tsbuildinfo\" \"apps/*/node_modules\"",
    "docker:s:up": "docker compose -f docker-compose.yaml --env-file apps/web/.env up -d",
    "docker:s:down": "docker compose -f docker-compose.yaml --env-file apps/web/.env down"
  },
  "devDependencies": {
    "@turbo/gen": "^1.12.0",
    "del-cli": "^7.0.0",
    "minimatch": "^10.1.1",
    "prettier": "^3.8.0",
    "turbo": "^2.7.5",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": ">=23"
  }
}
